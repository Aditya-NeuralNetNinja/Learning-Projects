#Imports
import numpy as np 
import pandas as pd 
from sklearn.datasets import load_diabetes

# Load dataset
data = load_diabetes()

# Convert to DataFrame
df = pd.DataFrame(data.data, columns=data.feature_names)

# Display DataFrame shape, types, and first few rows
print(df.shape)
print(df.dtypes)
print(df.head())

# Target variable
target = pd.DataFrame(data.target)




